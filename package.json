{
    "version": "1.0.0",
    "scripts": {
        "test": "jest --forceExit --runInBand",
        "schema:compile": "node compile-jsonschema.js",
        "build": "npm run schema:compile && tsc",
        "build:clean": "rm -rf dist && npm run build",
        "pg:migration": "knex migrate:make ${1}",
        "start": "node dist/bin/api-server.js",
        "dev": "tsc --watch & pm2-runtime start process.config.js",
        "watch": "tsc-watch --onCompilationStarted \"npm run schema:compile\" --onSuccess \"npm start\""
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@bahatron/utils": "^2.5.1",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.0.3",
        "@types/node": "^16.11.7",
        "axios": "^0.24.0",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "jest": "^27.3.1",
        "json-schema-to-typescript": "^10.1.5",
        "jsonschema": "^1.4.0",
        "knex": "^0.95.14",
        "pg": "^8.7.1",
        "ts-node": "^10.4.0",
        "tsc-watch": "^4.5.0",
        "typescript": "^4.4.4"
    }
}
